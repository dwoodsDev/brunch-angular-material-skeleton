body
  margin 0
  padding 0
  color #333
  background-color #fff
  font 1em/1.2 "Helvetica Neue", Helvetica, Arial, Geneva, sans-serif

button
  margin 2% 0

table
  width 100%

footer
  clear both
  color rgb(139, 139, 139)
  font-size 0.8em
  padding 5px 15px
  text-align center

#page
  padding 1%
  width 960px
  margin 5px auto

#header-logo
  width 28%
  float left
  padding 5px 1%

#ba-logo
  text-align center
  display flex
  flex-direction column
  align-items center
  padding 16px
  margin 0 auto
  width 90%

#header-info
  height 100px
  width 28%
  padding 5px 1%
  float left
  font-size 0.8em

#top-info
  border solid 1px rgb(139, 139, 139)
  background-color #d2d2d2
  text-align center
  padding 2%

  -webkit-border-radius 4px 4px 0 0
  -moz-border-radius 4px 4px 0 0
  border-radius 4px 4px 0 0

  -webkit-background-clip padding-box
  -moz-background-clip padding
  background-clip padding-box

#left-info
  border 1px solid rgb(139, 139, 139)
  border-top-width 0
  clear both
  margin 0
  float left
  width 45.3%
  padding 2%

  -webkit-border-radius 0 0 0 4px
  -moz-border-radius 0 0 0 4px
  border-radius 0 0 0 4px

  -webkit-background-clip padding-box
  -moz-background-clip padding
  background-clip padding-box

#right-info
  border 1px solid rgb(139, 139, 139)
  border-top-width 0
  border-left-width 0
  margin 0
  float left
  width 45.5%
  padding 2%

  -webkit-border-radius 0 0 4px 0
  -moz-border-radius 0 0 4px 0
  border-radius 0 0 4px 0

  -webkit-background-clip padding-box
  -moz-background-clip padding
  background-clip padding-box

.md-action-response
  max-height 36px

  div
    margin auto
    margin-right 10px
  img
    max-height 36px

.md-breadcrumb
  color rgb(139, 139, 139)
  font-size 1.5em
  font-weight 400 !important
  height 100px

.md-whiteframe-glow-z1
  box-shadow 0px 0px 1px 1px rgba(0, 0, 0, 0.14),
              0px 0px 2px 2px rgba(0, 0, 0, 0.098),
              0px 0px 5px 1px rgba(0, 0, 0, 0.084)

.tab-label
  font-weight bold

.config-menu
  list-style-type none
  margin 0
  padding 0

.config-menu:first-child
  border-top 1px solid rgb(224, 224, 224)

.config-menu > li
  border-bottom 1px solid rgb(224, 224, 224)

.config-saved-check
  width 22px

.nav-heading
  background rgb(139, 139, 139)
  color #ffffff
  font-size 1em
  margin 0 0 5px 0
  padding 2px
  text-transform uppercase

.menu-icon
  color rgb(90, 90, 90) !important

.menu-item a
  color rgb(90, 90, 90)
  display block
  font-size large
  font-weight normal
  height 100%
  line-height 32px
  margin 0px
  max-width 100%
  overflow-x hidden
  padding 8px 16px 0
  text-align left
  width 100%
  text-transform none

.menu-item a:link, a:visited
  color rgb(90, 90, 90)
  text-decoration none

.menu-item:hover
  background-color rgb(246, 246, 246)

.menu-item.active
  background-color rgb(246, 246, 246)

#main
  width 98%
  padding 5px 1%
  float left

#main-title
  font-size 1.5em
  font-weight bold
  margin 0 0 5px

.bold-label
  font-weight bold
  font-size large

.text-input-label
  font-weight bold
  font-size 1em
  margin 0 2% 2% 0

.label-col
  clear both
  float left
  width 25%

.layout-column
  flex-direction column

.mode-col
  float left
  width 25%

.interface-col
  float left
  width 25%

.filter-col
  float left
  width 25%

.vlan-head
  float left
  font-weight bold
  width 25%

.vlan-clear
  clear both

.vlan-col
  float left
  width 25%

.radio-radio
  margin 5px 0

.radio-label
  float left
  width 25%

.radio-float
  float left

.radio-clear
  clear both

.checkbox-label
  margin auto

.client-label
  float left
  margin 0 1% 0 0

.client-float
  float left

.client-boot-checkbox
  float left
  margin 0 0 0 5%

.client-checkbox-label
  float left
  margin 0 5% 0 0

.client-clear
  clear both

.client-section-label
  margin 0 0 20px 0

.client-head
  float left
  font-weight bold
  width 25%

.client-col
  float left
  width 25%

.save-config-button
  margin 20px
  width 100%
  color rgb(64,180,229) !important
  background-color rgb(246, 246, 246) !important

.save-config-button:hover
  background-color rgb(239, 239, 239) !important

.save-config-button-toast
  position relative
  margin auto
  padding-left 10px
  padding-right 0
  min-width 236px
  max-width 236px
  width 236px

.section-head
  float left
  font-weight bolder

.site-content-toolbar
  background #f6f6f6 !important
  color #202020 !important
  z-index 3

.site-sidenav
  width 256px !important
  min-width 256px !important
  max-width 256px !important

.arinc-clear
  clear both

.arinc-float
  float left

.arinc-enabled
  float left
  font-weight bold
  margin 0 3% 0 0

.arinc-speed-label
  float left
  margin 0 1% 0 0

.arinc-speed
  float left
  margin 0 3% 0 0

.arinc-input
  float left
  width 9%

.arinc-col
  float left
  width 50%

.arinc-inner-col
  float left
  margin 2% 2% 2% 0
  width 45%

.bearer-enabled-checkbox
  min-width 140px

.bearer-head
  float left
  font-weight bold
  width 33%

.bearer-clear
  clear both

.bearer-col
  float left
  margin 1% 0 0 0
  width 33%

.bearer-float
  float left

.bearer-input
  float left
  margin 1% 0 0 0

.bearer-label
  float left
  margin 1% 2% 0 0

.device-card-title
  padding 0 20px

.jaguar-toolbar
  padding 0 0 0 15px

.fab
  position fixed
  bottom 0
  right 0
  width 110px
  height 80px
